      <div class="modal-header">
        <h4 class="modal-title"></h4>
        <button type="button" id="closeBtn" class="close" data-dismiss="modal">&times;</button>
      </div>
      {{!-- Body --}}
      <div class="modal-body text-center">
        <form class="note-form">
          <div class="form-note">
            <h3>Add a New Note</h3>
            <input type="text" name="articleNote" id="artNote" required="required">
            <br>
          </div>
          <br>
          <button type="submit" id="submitBtn" class="btn btn-success">Submit</button>
        </form>
      </div>
      <div class="modal-footer">
        <button id="modalclosebutton" type="button" class="btn btn-default" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
